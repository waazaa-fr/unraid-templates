  <Config Name="/app/storage" Target="/app/storage" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/zendo/config/</Config>
  <TailscaleStateDir/>
</Container>
